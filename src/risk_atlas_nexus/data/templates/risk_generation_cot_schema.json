{
    "anyOf": [
        {
            "type": "object",
            "patternProperties": {
                "^.*$": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "Input": {
                                "type": "string"
                            },
                            "Output": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        },
                        "required": [
                            "Input",
                            "Output"
                        ],
                        "additionalProperties": false
                    }
                }
            }
        },
        {
            "type": "null"
        }
    ]
}
