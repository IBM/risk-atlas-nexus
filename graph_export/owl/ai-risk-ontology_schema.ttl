            owl:onProperty nexus:hasLicense ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:bestValue ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:hasUnitxtCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom nexus:AiEval ;
